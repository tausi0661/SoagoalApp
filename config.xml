<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="org.zsyz.gjb.soagoalapp" version="1.0.1">
  <name>Soagoal家校通-中山一中国际部</name>
  <description>Soagoal家校通-中山一中国际部</description>
  <author href="http://gjb.zsyz.org/soagoalapp" email="soagoal_gjb@163.com">Soagoal Team</author>
  <content src="index.html"/>
  <preference name="permissions" value="none"/>
  <preference name="orientation" value="portrait"/>
  <preference name="target-device" value="handset"/>
  <preference name="fullscreen" value="false"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="prerendered-icon" value="true"/>
  <preference name="stay-in-webview" value="false"/>
  <preference name="ios-statusbarstyle" value="black-opaque"/>
  <preference name="detect-data-types" value="true"/>
  <preference name="exit-on-suspend" value="false"/>
  <preference name="show-splash-screen-spinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="true"/>
  <preference name="disable-cursor" value="false"/>
  <preference name="android-minSdkVersion" value="7"/>
  <preference name="android-installLocation" value="auto"/>
  <plugin name="org.apache.cordova.battery-status"/>
  <plugin name="org.apache.cordova.camera"/>
  <plugin name="org.apache.cordova.media-capture"/>
  <plugin name="org.apache.cordova.console"/>
  <plugin name="org.apache.cordova.contacts"/>
  <plugin name="org.apache.cordova.device"/>
  <plugin name="org.apache.cordova.device-motion"/>
  <plugin name="org.apache.cordova.device-orientation"/>
  <plugin name="org.apache.cordova.dialogs"/>
  <plugin name="org.apache.cordova.file"/>
  <plugin name="org.apache.cordova.file-transfer"/>
  <plugin name="org.apache.cordova.geolocation"/>
  <plugin name="org.apache.cordova.globalization"/>
  <plugin name="org.apache.cordova.inappbrowser"/>
  <plugin name="org.apache.cordova.media"/>
  <plugin name="org.apache.cordova.network-information"/>
  <plugin name="org.apache.cordova.splashscreen"/>
  <plugin name="org.apache.cordova.vibration"/>
  <plugin name="com.indigoway.cordova.whitelist.whitelistplugin" spec="1.1.1" source="pgb" />
  <icon src="icon.png"/>
  <icon src="res/icon/applogoicons/36.png" platform="android" qualifier="ldpi"/>
  <icon src="res/icon/applogoicons/48.png" platform="android" qualifier="mdpi"/>
  <icon src="res/icon/applogoicons/72.png" platform="android" qualifier="hdpi"/>
  <icon src="res/icon/applogoicons/96.png" platform="android" qualifier="xhdpi"/>
  <icon src="res/icon/applogoicons/ios57.png" platform="ios" width="57" height="57"/>
  <icon src="res/icon/applogoicons/ios72.png" platform="ios" width="72" height="72"/>
  <icon src="res/icon/applogoicons/ios76.png" platform="ios" width="76" height="76"/>
  <icon src="res/icon/applogoicons/ios114.png" platform="ios" width="114" height="114"/>
  <icon src="res/icon/applogoicons/ios120.png" platform="ios" width="120" height="120"/>
  <icon src="res/icon/applogoicons/ios144.png" platform="ios" width="144" height="144"/>
  <icon src="res/icon/applogoicons/ios152.png" platform="ios" width="152" height="152"/>
  <splash src="res/screen/android/screen-ldpi-portrait.png" platform="android" qualifier="port-ldpi"/>
  <splash src="res/screen/android/screen-mdpi-portrait.png" platform="android" qualifier="port-mdpi"/>
  <splash src="res/screen/android/screen-hdpi-portrait.png" platform="android" qualifier="port-hdpi"/>
  <splash src="res/screen/android/screen-xhdpi-portrait.png" platform="android" qualifier="port-xhdpi"/>
  <splash src="res/screen/ios/screen-iphone-portrait.png" platform="ios" width="320" height="480"/>
  <splash src="res/screen/ios/screen-iphone-portrait-2x.png" platform="ios" width="640" height="960"/>
  <splash src="res/screen/ios/screen-iphone-portrait-568h-2x.png" platform="ios" width="640" height="1136"/>
  <splash src="res/screen/ios/screen-ipad-portrait.png" platform="ios" width="768" height="1024"/>
  <splash src="res/screen/ios/screen-ipad-landscape.png" platform="ios" width="1024" height="768"/>
  <access origin="*"/>
  <allow-intent href="*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <allow-intent href="*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>